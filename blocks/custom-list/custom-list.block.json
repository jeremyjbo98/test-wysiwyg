{
    "name": "custom-list",
    "label": "Lista Personalizada",
    "type": "group",
    "wysiwyg": true,
    "items": {
      "listItems": {
        "type": "array",
        "label": "Elementos de la lista",
        "itemLabel": "{{title}}",
        "items": {
          "title": {
            "type": "text",
            "label": "Título"
          },
          "description": {
            "type": "textarea",
            "label": "Descripción"
          },
          "icon": {
            "type": "asset",
            "label": "Ícono"
          }
        }
      }
    }
  }
  